			@include link-colors(#4D769F, #3399cc, #4D769F, #4D769F);
			text-decoration:none;
		}
		a.title{
			display:block;
			padding:0px;
			font-weight:normal;
			padding-bottom:4px;
			font-size:13px;
			position:relative;			
		}
	}
	div.list-item{
		margin-bottom:4px;
		margin-left:-22px;
	}
}

.forum_list{
	margin:0px 0px;
	a{ text-decoration:none; }
	a.title{ @include link-colors(#545454, #527EAA, #545454, #545454); }
	.item_actions{
		float:right;
		font-size:10px;
		background-color:#F3F1EC;
		@include corners(6px);
		border:1px solid #cfcfcf;
		color:#afafaf;
		padding:2px 8px;
		a.highlight{
			background-color:#ffffcc;
			padding:1px 3px;
		}
	}
	.subcounter{ 
		color:#afafaf;
	}
	ul.base-set{
		@include no-bullets;
		@include clearfix;
		margin:0px; 
		padding:5px 26px;
		li{
			float:left;
			width:44%;
			padding:0px 0px 0px 0px;
			margin:0px 3% 0px 0px;
			h4.forum_title{
				margin:8px 0px 0px 0px;
				padding:6px 0px 8px 33px; 
				font-size:16px;
				a{
					font-weight:normal;
				}
				position:relative;
			}
			span.icon{
            position:absolute;
            left:3px; top:-1px;
            width:32px; height:32px;
			} 
			span.forum_type_howto{ background: sprite($icons, forum_howto) no-repeat; }
			span.forum_type_ideas{ background: sprite($icons, forum_ideas) no-repeat; }
			span.forum_type_problem{ background: sprite($icons, forum_problem) no-repeat;	}
			span.forum_type_announce{ background: sprite($icons, forum_announce) no-repeat; }
			.topic-list{ 
				padding:6px 0px 12px 0px;
				margin-left:34px;
				margin-bottom:8px;
				border-top:1px solid #efefef;
				a.topic{
					display:block;
					padding:5px 0px;
					font-size:13px;
				}
				a{ 
					@include link-colors(#4D769F, #3399cc, #4D769F, #4D769F);	
				}
			}
		}
		li.odd{
			clear:left;
		}
		li.even{
			margin-right:0px;
			width:46%;
		}
	}
	
}

.topic-list-view{ 
		padding:0px;
		margin:0px 0px;
		border:1px solid #f1f1f1;
		border-width:1px 0px; 
		li{
			@include clearfix;
			border-bottom:1px dotted #eaeaea;
			padding:14px 25px 10px 70px;
			position:relative;
			a{ 
				@include link-colors(#4D769F, #3399cc, #4D769F, #4D769F);
				text-decoration:none;  
			}
			a.topic-title{  
				font-size:14px; 
				font-weight:500;
			}
			.info_data{
				padding:6px 0px 4px 0px;
				font-size:12px;
				color:#888888;
			}
			.preview_pic{
				border-color:#d2d2d2;
				position:absolute;
				left:20px;
				top:16px;
				img{ width:36px; height:36px; padding:0px; border:0px; }
			}
		}
		li:hover{
			background:#f4f4f4;
		}
}

.page-list{
   margin: 0 -20px;
   .list-group{
      padding:0px;
      margin:0px 0px;
      border:1px solid #dfdfdf;
      border-width:1px 0px;
      .list-item{
         @include clearfix;
         border-bottom:1px dotted #eaeaea;
         padding:14px 25px 6px 70px;
         position:relative;
			.primary-text{  
				font-size:13px; 
				line-height: 19px;
				padding: 2px 0px 6px;
				color:#444;
			}
			.info-text{
				padding:6px 0px 4px 0px;
				font-size:12px;
				color:#888888;
			}
			.preview-pic{
				border-color:#d2d2d2;
				position:absolute;
				left:20px;
				top:16px;
				img{ width:36px; height:36px; padding:0px; border:0px; }
			}
			.username{ 
			   padding:0px 0px 3px 0px;
			   font-weight:bold;
			   font-size:13px;
			}
			.item_actions{
			   visibility:hidden;
			}
			.info-data{
			   a{ 
			      text-decoration:none;
			      padding-left:8px;
			      margin-left:6px;
			      border-left:1px solid #cfcfcf;
			   }
			   .floatr{
			      margin-top:-5px;
			   }
			}
      }
      .list-item:hover{
         background-color:#e6f3f9;
   		background-image:url(../images/overlay-top-shadow.png);
   		background-position:top left;
   		border-bottom:1px solid #dfdfdf;
   		background-repeat:repeat-x;
   		.item_actions{
   		   visibility:visible;
   		}
      }
      .loadingbox{
         background-image:url('/images/load.gif');
   	   background-repeat:no-repeat;
   	   background-position: center center;
   	   height:200px;
      }
   }
}

.goto_ticket{ display:none; }

.tweet_converted{
   background-color:#ffffee;
   .convert_as_ticket{ display:none; }
   .goto_ticket{ display:inline-block; }
}

.tweet_converted:hover {
   background-color:#ffffdd !important;   
}

.results-info{
   font-style:italic;
   font-size:13px;
   padding:6px 18px;
   color:#999999;
   background:#f8f8f8;
}

#tweet_noresults{
   height:200px;
}

.left-panel{
   width:150px;
   padding:6px 20px;
   background-color:#f8f8f8;
   background-image:url(../images/overlay-right-shadow.png);
	background-position:top right;
	background-repeat:repeat-y;
}

.tweet-table{
   border:1px solid #eaeaea;
   border-width:1px 0px;
   width:100%;
}

.search-keys{
   margin:0px 0px 8px 0px;
   li{
		padding:3px 8px 5px 18px;
		position:relative;
		span.icon{
		   background:sprite($symbol, search) no-repeat;
		   position:absolute;
		   left:0px;
		   width:15px; height:15px;
	   }
	   a{
	      font-weight:bold;
	   }
	   a.selected{ 
	      color:#3399CC;
	      text-decoration:none;
	   }
   }
}

.more_results{
   padding:8px 12px;
   background-color:#f4f4f4;
}

.info-results{
   font-size:1.26em;
   padding:10px 20px;
   color:#666666;
   background-color:#f6f6f6;
   border-top:1px solid #f0f0f0;
}

.admin_help_image{
   margin:30px 15px;
   @include clearfix;
   .help_image{
      float:left;
   }
   .content{
      margin:0px 0px 0px 230px;
      p.info-data{
         font-family:verdana;
         font-size:15px;
         line-height:26px;
         padding-right:8%;
      }
   }
}

.tweet-counter{
   font-size:16px; 
   position:relative;
   top:3px;
   right:5px;
   color:#333333;
}

.tweet-counter.error{
   color:#FF0000;
}

.adminTitle{
   font-size:20px;
   color:#333;
   text-transform:uppercase;
   font-weight:bold;
}

.loading-amount{
   background-image:url('/images/load.gif');
	background-repeat:no-repeat;
	background-position: center;
   table{
      visibility:hidden;
   }
}
 
.quoted_button{
  margin:0 0;
  background:sprite($symbol, quote_button) no-repeat;
  display:inline-block;
  width:31px;
  height:14px;  
}

.customSelect{ width:100%; } 
   
.containerFull{
   @include clearfix;
   overflow:visible;
	margin:-14px -20px;
	padding:0px 0px 0px 0px;
}

.greyImage{
   background-color:#F1F1F1;
}

.drop-right{
   padding:2px 18px 2px 6px;
   margin-left:-6px;
   color:#666666;
   @include corners(4px);
   text-decoration:none;
}

.drop-right:after{
   content:"";
   position:absolute;
   width:13px; height:12px;
   margin-top:2px; margin-left: 3px;
   background: sprite($symbol, drrow-down) no-repeat;   
}

.drop-right:hover, .drop-right.selected{
   color:#ffffff;
   *{ color:#ffffff; }
}

.drop-right:hover{
   background-color:#afafaf;   
}

.drop-right.selected{
   background-color:#888888;
   @include single-box-shadow(rgba(0, 0, 0, 0.6), 0px, 1px, 2px, false, true);
}

.ticket-leftcol{
   width:230px;
   float:left;
	@include min-height(477px);
   border:1px solid #ffffff; 
   border-width:1px 1px 0 0;
	@include linear-gradient(color-stops(#f4f4f4, #F1F1F1), top);
	
   h3.title{
      color:#888888;
      font-size:12px;
      text-transform:uppercase;
      padding:8px 8px 4px 8px;
      margin:0;
   }
   h4.title{
      padding:0 6px 3px 0;
      font-size:11px;
		text-transform:uppercase;
      color:#666666;
   }
   
   .view_filters{
		.link-item.active, a.link-item:hover{
         margin:0px -5px;
         padding-left:13px;
         background: #e2f1fa;
		}
		.link-item.active{
		   color:#336699;
         font-weight:bold;
         @include linear-gradient(color-stops(darken(#e2f1fa, 2%), lighten(#e2f1fa, 2%)), bottom);
		   @include single-box-shadow( rgba(0, 0, 0, 0.07), 0px, 0px, 2px, false, true);         
		}
		.link-item.active:after{
		   content:"";
		   position:absolute;
		   right:9px;
		   top:8px;
		   width:11px;
		   height:11px;
		   background: sprite($symbol, arrow-selected-right) no-repeat;
		}
      .link-item{ 
         font-size:13px;
         display:block;
         color:#444444;
         text-decoration:none;
         padding:5px 8px;
         position:relative;
      } 
   }
   
   .leftFilter{
		$bordercolor:#cfcfcf;
		@include corners(3px);		
   	background: #fafafa;
		@include single-box-shadow(#ffffff, 0px, 1px, 1px);
		margin:6px;
   	padding:5px;
		position:relative;
		border:1px solid $bordercolor;  
		border-bottom-color:darken($bordercolor, 5%);
		border-right-color:darken($bordercolor, 5%);
   	.checkboxSet{
   		margin-bottom:10px;
   	}
   	#save_filter{
         position:relative;
         top:4px;
         left:2px;
         display:none;
      }
   	.ff_item{
         margin:0 6px 4px 6px;
			padding:4px 2px;
			border-bottom:1px dotted #cfcfcf;
      }
   	.item{
   		padding-bottom:2px;
			padding-top:2px;
   		font-size:12px;
   		color:#444444; 
   		input{margin:0 4px 3px 0; }
   	} 
		.item:hover span{
			background:#ffffcc;
			color:#000000;
		}
   }
}

.hasRightCol{
   margin-left:231px;
} 

.page_info{
   float:right;
   width:200px;
   text-align:right;
   margin-right:6px;
   position:relative;
}

.contentCol{
   border-left:1px solid #cfcfcf;
   background-color:#ffffff;
}

.toolbar{
   background:#f4f4f4;
	padding:5px 8px 8px 0;
	border-bottom:1px solid #dfdfdf;
	border-top:1px solid #ffffff;
}

.sort-bar{
	font-size:11px;
	color:#777777;
	padding: 5px 10px 5px 40px;
	b{ color:#666666; }
}


.fd-menu{
   position:absolute;
   display:none;
   font-size:13px;
   background:#ffffff;
   padding:5px 0;
   z-index:999999;
   min-width:200px;
   @include opacity(0.95);
   @include corners(5px);
   @include single-box-shadow( rgba(0, 0, 0, 0.6), 0px, 2px, 4px );
   li{ padding:0px; margin:0px; }
   .seperator{ border-color:#e0e0e0; }
   a{
      display:block;
      padding:2px 12px 2px 24px;
      color:#333333;
      text-decoration:none;
      position:relative;
      .icon{
         position:absolute;
         left:5px;
         top:2px;
         width:16px;
         height:16px;
      }
      .ticksymbol{ background: sprite($symbol, selected) no-repeat; }      
   }
   a:hover{
      @include background(linear-gradient(top, #5469f9, #2433f6));
      @include single-box-shadow( rgba(0, 0, 0, 0.6), 0, 0px, 1px, false, true );
      @include filter-gradient(#5469f9, #2433f6, vertical); 
      color:#ffffff;
      .selected{ background: sprite($symbol, selected-negtive) no-repeat; }      
   }
}

.setting_bottom{
   position:absolute;
   right:7px;
   bottom:10px;
   width:16px;
   height:16px;
   background:sprite($symbol, setting) no-repeat;
}

.content .wrapper{
   overflow:visible;
}

.list-noinfo{
	padding-top:60px; 
	font-size:1.4em;
