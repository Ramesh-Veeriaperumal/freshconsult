@import "base";
@import "mixins";

/* Importing admin icons */
@import "admin-icon/*.png";
@import "application-logo/*.png";

/* Bullet icon */ 
$bullet-icon:"symbols/request_nav_seperator.gif";
body { color: #333; background:$background; min-width:$blueprint-liquid-container-min-width+20; }

/* PAGE AREA STYLE */ 

.sp_border{
	border:1px solid darken($background, 15%);
}

.pagearea, .sidepanel, .rtCnt, .no-helpdesk{
	background-color:#ffffff;	
}

.pagearea, .sidepanel, .rtCnt, .no-helpdesk, #errorExplanation, .flash_info{  
   @extend .sp_border;
	padding:14px 20px; margin:0px 10px 10px 0px;
}

.sidepanel, .rtCnt{
	@include shadows(rgba(0,0,0, 0.05), 0px, 1px, 2px);
} 

.pagearea{
	@include min-height(450px);
	@include shadows(rgba(0,0,0, 0.2), 0px, 2px, 3px);
	position:relative;
}

ul.activityfeed, ul.summary{
	list-style:none;
	padding:0px;
}

ul.summary{
// @include pie;
	@include corners(6px);
	@include inset-shadows(#ececec, 1px, 1px, 2px);
	background-color:#f7f7f7; border:1px solid #f7f7f7;
	margin-bottom:10px; 
	li{ 
		@include float-left; 
		margin:2px 0px;
		padding:0px 0px 4px 0px; width:106px;
		border-right:1px solid #e8e8e8; text-align:center;
		a{ 
			text-decoration:none; color:#333333; text-align:center;
			span{
				display:block;
				position:relative;
				top:8px;
			}
			h4{			 
				font-size:36px; color:#666666;
				padding-top:8px;	
				font-weight:bold; 
			}
		}	
		a.overdue{
			color:#996666;
			h4{ color:#a65252; }
		}
	}	
	li.last { border:0px; }		 
}  

ul.activityfeed li{ 
	border-bottom:1px solid #f7f7f7;
	padding:10px 20px 8px 66px;
	display:block; 
	color:#666666;
	font-size:1.1em; line-height:1.6;
