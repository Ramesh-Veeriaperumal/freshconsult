/* Sharp Fold Corners */


.ltSlant, .rtSlant{
	  width: 0;
	  height: 0;
	  position: absolute;  
	  overflow: hidden; /* IE fix */ 
}

.ltSlant{
	  border-left: 11px solid #FFF; /* IE fix */
	  border-left: 11px solid rgba(0,0,0,0); 
}
.rtSlant{	 
	  border-right: 11px solid #FFF; /* IE fix */
	  border-right: 11px solid rgba(0,0,0,0); 
}

.title_corner{
	left:-10px;
	top:0px;
	position:absolute;
	width:10px;
	height:100%;
	background:#ecedf3;
	span.ltSlant{
		top:-10px; 
		border-bottom-color:#c5c8dc;
	}
}

