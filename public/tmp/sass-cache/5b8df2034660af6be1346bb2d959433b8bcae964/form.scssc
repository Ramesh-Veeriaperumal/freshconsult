	@include clearfix;
	display:block;
	margin:1px 0;
	padding:2px;
	border:1px dotted #cfcfcf;
	border-right-color:#e2e2e2;
	border-bottom-color:#e2e2e2;
	background-color:rgba(0, 0, 0, 0.01);
	display:block;
	
} 

.dialog-panel{
	position:absolute;
	width:320px;
	z-index:999999;
	@include shadows(0, 2px, 4px, rgba(0, 0, 0, 0.5));
    @include vertical-gradient(#f5f5f5, #e9e9e9);
    @include corners(4px);
/*	@include pie;*/
}


.border-top{
	margin-top:6px;
	padding-top:6px;
	border-top:1px dotted #cfcfcf;
}

.overlay-field{
	position:absolute;
	width:100%;
	height:100%;
	background:none;
}

.code-view{
	padding:8px 20px;
	textarea{
		border:1px dotted #afafaf;
		display:block;
		background:#ffffff;
		font-size:12px;
		color:#333333;
		height:160px;
		font-family: "courier";
	}
}
.small_select{
	font-size:11px;
	width:110px;
}

.due-date-dialog{
	position:absolute;
	left:0;
	top:0;
	margin-left:-20px;
	margin-top:-6px;
	z-index:99999;
	width:250px;
 	@include shadows(0, 2px, 4px, rgba(0, 0, 0, 0.5));
    @include vertical-gradient(#f5f5f5, #e9e9e9);
    @include corners(4px);
	padding:8px 10px;
    border:1px solid #e0e0e0;
	.due-date-calender{ 
		@include vertical-gradient(#f5f5f5, #e9e9e9);
	  	padding:4px 10px;
	  	margin:4px 0 8px 0;	
	  	@include corners(4px);
	  	border:#e0e0e0 1px solid;
	}
	.right_pointer{
	 	background: sprite($symbol, calender-pointer-right) no-repeat;
		position:absolute;
		width:10px; height:18px;
		right:-10px;
		top:10px;
	}
	.ui-datepicker-inline{ margin:0 0 8px 0;}
	.ui-corner-all{	@include corners(0); }
	.ui-widget-content, .ui-widget-header{ background:none; border:0; }
	.ui-datepicker .ui-datepicker-title select, .due-date-calender select, #due-date-options{ font-size: 11px; width:auto; margin-right:4px; }
	.ui-widget-content{ font-size: 13px; }
	.ui-datepicker-prev, .ui-datepicker-next{ cursor:pointer; }
	.ui-datepicker-prev:hover, .ui-datepicker-next:hover{ background:#f4f4f4; }	
	.ui-datepicker-prev .ui-icon{ background: sprite($symbol, calender-left) no-repeat; }
	.ui-datepicker-next .ui-icon{ background: sprite($symbol, calender-right) no-repeat; } 
	.ui-datepicker th { font-weight: normal; }
	.ui-datepicker-inline th, .ui-datepicker-inline td a{ text-align:center; color:#333333;  }
	.ui-datepicker-inline td .ui-state-default{ border:0; background:none; }
	.ui-datepicker-inline td .ui-state-default:hover{ background-color:darken(#dde8ee, 5%); }	
	.ui-datepicker-inline td .ui-priority-secondary{ color:#888888; }
	.ui-datepicker-inline td .ui-state-highlight{ background:#FFFFCC; color:#333333;}
	.ui-datepicker-inline td .ui-state-active, .ui-datepicker-inline td .ui-state-active:hover{ background:#3F3F3F; color:#ffffff; }
	.due-date-info p{ 
		text-align:center; 
		font-size:13px; 
		margin:0; 
		padding:6px 0;
	}
	
	.ui-widget-header{
		font-size:14px;
  		color:black;
	}
	.calender-buttons{
		position:relative;
		text-align:right;
		padding:12px 5px 6px 5px;
		margin-top:10px;
		border-top:1px solid #e0e0e0;
		span.loading-item{
			width:16px;
			height:11px;
			position:absolute;
			right:15px;
			margin-top:12px;
			background-image:url('/images/load-fb-style.gif');
			background-repeat:no-repeat;
			visibility:hidden;
			background-position: 0 0;
		}
	}
	.saving-items{
		.uiButton{ visibility:hidden; }
		span.loading-item{ visibility:visible; }
	}
}

.transparent-overlay { position: absolute; top: 0; left: 0; width: 100%; height: 100%; z-index:99998; background:#ffffff; @include opacity(0); }

.tabbed{ padding-left:25px; }

table.summary{
	width:100%;
	td{ 
		padding:8px 10px 2px 0;
	}
}
table.summary-report{
	width:100%;
	border:1px solid #ececec;
	border-width:1px 0;
	font-size:13px;
	td, th{ 
		padding:8px 8px;
		width:180px;
		border-bottom:1px solid #eaeaea;		
	}
	th{
		background:#ededed;
		font-weight:bold;
		color:#333333;
	}
	.sub-title{
		background:#f1f1f1;
		text-align:right;
		width:280px;
		padding-right:10px;
		text-transform:capitalize;
	}
	thead{
		margin:0 0 0 0;
		color:#666666;
		border:1px solid #f4f4e9;
		background-color:#fffbee;	
		td{
			text-align:center;
			font-weight:bold;	
			padding:12px 18px;
		}
	}
}



.email-settings{ 
	margin:0 -20px 10px;
	padding:0;
	border-top:1px solid #e6e6e6;
	border-bottom:1px solid #f4f4f4;
	label{
		font-size:14px;
		color:#666666;
		display:block; 
	}
	.action_buttons{
		margin-top:2px;
	}
	.highlight{ 
		font-size:inherit; 
		font-size:15px;
	}
	.info_data{ font-size:12px; }
	
	div.email-item-default, div.email-item{
		padding:6px 22px 4px;
		background:#f6f6f6;
	}
	
	div.email-item-default{
		label{ 
			line-height:20px;
		}
		padding-top:10px;
		padding-bottom:10px;
		.info_data{
			padding-top:4px;
		}
	}			
	div.email-item{
		border-top:1px solid #ffffff;
		border-bottom:1px solid #e8e8e8;
		div.item{
			margin-bottom:8px;
			label{ font-size:12px; padding-bottom:2px; }
			font-size:14px;
		}	
	
	}
} 
.email-inactive{
	.highlight{
		color:#777777 !important;
	}	
}

.email-settings-form{
	table{ width: 400px; }
	td{ padding:6px 8px;}
}

.inactive_email{
	padding:2px 5px;
	margin-left:15px;
	border:1px solid darken(#f7e6e6, 5%);
	background-color:#f7e6e6;
	color:#b56e6e;
	font-size:11px;
	position:relative;
	top:-1px;
	text-transform:capitalize;
	font-weight:bold;
	@include corners(5px);
	a{
		font-size:10px;
	} 
}

#primary-email-settings{
	div.email-item-default, div.email-item{
		background-color:#f3fde5;
	}
}

.highlight-textbox-info{
	font-size:16px;
	color:#0078BD;
	width:96%;
	font-weight:400;
	border:1px solid #CFCFCF;
	background-color:#efefef;
	padding:5px 3px 4px 5px;
	height:18px;
}

.email_info{
	border:1px solid #cfcfcf;
	padding:8px 12px;
	background:#f2ffda;
	@include corners(3px);
	font-size:14px;
}

.email-info{
	background-color:#fefce5;
	padding:12px 22px;
	line-height:22px;
	margin:0 -20px;
	font-size:13px;
	color:#996666;
	border:1px solid #f4f4f4;
	margin-bottom:10px;
}

.request_panel{
   padding:0 0;
   margin:5px 0;
   .ui-form{
      li.field{
         border:0;
      }
      label, div.fields label{
         font-size:13px;
         color:#333333;
      }
      label.overlabel{
         margin:2px 8px;
         color:#888888;
         font-size:12px;
      }
      li.inline-field{
         padding:1px 18px;
         label.caption{
            width:80px;
         }
         div.fields{
            margin-left:86px;
            padding-top:4px;
            position:relative;
            .info-data{
               margin:8px;
            }
         }
         input.text, textarea{
            width:98%;
         }
         div.action_buttons{
            top:-6px;
            right:5px;
         }
      }
      .request_form_options{
         position:absolute;
         right:26px;
         top:20px;
         .insert-canned-response{
            position:relative;
            z-index:1001;
         }
      }
      .button-container{
         padding-top:8px;
         margin-top:8px;
         border-top:1px solid #efefef;
      }
   }
}

.nav_search{
	@include min-width(260px);
	height:31px;
	position:relative;
	margin:0 11px 0 6px;
	div.search_bar{ 
		height:24px; 
		@include corners(12px);
		@include opacity(0.8);
		background:#ffffff;
		padding:0 25px 0 5px;
		.searchIcon{
		   position:absolute;
		   right:0px;
		   width:15px;
		   height:15px;
		   top:8px;
		   right:5px;
		   background: sprite($symbol, search);
		   @include opacity(0.6);
		}
		label.overlabel, input{
			top:4px;
			left:2px;
		}
		label.overlabel{
		 	padding:0;
		 	margin:4px 6px;
		 	font-size:13px;
		 	color:#777777;
		} 
		input{		
			border:0;
			position:relative;
			font-size:14px;
			width:100%;
			padding:0;
			background:none;		
		}
		input:focus{ @include shadows(#ffffff, 0, 0, 0); }		
	}
	div.search_bar.active{
	   @include opacity(1);
	   .searchIcon{
	      @include opacity(1);
	   }
   }
}

.nav_right{
	@include clearfix;
	position:relative;
	z-index:10; 
	margin:1px 0 0 3px;
	float:right; 
}

a.new_ticket, .nav_search{
	float:left;
}

a.new_ticket, .nav_search div.search_bar{
	border:1px solid darken($tab_color, 8%);
	@include single-box-shadow(rgba(255, 255, 255, 0.4), 0px, 1px, 0px);
	margin-top:2px;
	@include corners(4px);
} 
 
a.new_ticket{
 	@include vertical-gradient(#fefefe, #dfdfdf);	
	padding:5px 8px 4px 28px;
	color:#333333; 
	font-size:12px; 
	text-decoration:none; font-weight:bold;
	position:relative; 
	@include inline-block;	
	span { 
		position:absolute; left:4px; top:4px;
		background:sprite($symbol, add) no-repeat;
		width:16px; height:16px;
	}	
}

a.new_ticket:hover{ 
	@include vertical-gradient(#606c88, #3f4c6b);
	border-color:#606c88;  
	color:#ffffff;	 
}

.item-right-actions{
	@include pie-clearfix;
	float:right;
	position:relative;
	top:-6px;
}

.menu-box{
	border:1px solid #cecece;
	background-color:#ffffff;
	right:0;
	top:24px;
	padding:6px 4px;
	@include min-width(230px);
	@include shadows(rgba(0,0,0, 0.3), 0, 1px, 1px);
	position:absolute;
	z-index:99999;
	display:none;
	.menu-item{
		display:block;
		a{
			display:block;
			padding:4px 16px;
			text-decoration:none;
		}
		a:hover{
			background-color:#555555;
			color:#ffffff;
		}
	}
}
 
.nav-drop{ 
	.nav-item-icon{
		@include clearfix;
		float:left;
		border:1px solid white;
		span{
			width:23px;
			height:23px;
			margin:0 3px;
			float:left;
		}
		.item-options{
			background:sprite($symbol, item-options) no-repeat;
		}
	}	
	.nav-item-icon:hover, a.selected{
		border:1px solid #cdcdcd;
		@include vertical-gradient(#fbfdfc, #eaeaea);
	}
}

