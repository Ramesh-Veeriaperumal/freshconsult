<%
	var title_name = HelpdeskReports.Constants.CustomerReport.metrics[metric].title;
%>

<div class="pdf-chart-container">
	<div class="title">
	</div>
	<div class="pdf-title-holder">
		<span class="left-title"><%= title_name %> (<%= I18n.t('helpdesk_reports.customer_report.top_5') %>)</span>
		<span class="right-title"><%= title_name %> (<%= I18n.t('helpdesk_reports.customer_report.bottom_5') %>)</span>
	</div>
	<div class="pdf-chart-holder">
		<div id="<%= metric %>_desc_container" class="half-container" data-metric="<%= metric %>" data-report="customer-report-container"></div>
		<div id="<%= metric %>_asc_container" class="half-container" data-metric="<%= metric %>" data-report="customer-report-container"></div>
	</div>
	<br/>
</div>
