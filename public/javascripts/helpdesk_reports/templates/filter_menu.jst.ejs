<% for (var i in data) { %>
	<% if (HelpdeskReports.CoreUtil.global_disabled_filter.indexOf(data[i].condition) < 0 && HelpdeskReports.CoreUtil.default_available_filter.indexOf(data[i].condition) < 0 && (HelpdeskReports.CoreUtil.reports_specific_disabled_filter[HelpdeskReports.locals.report_type] || []).indexOf(data[i].condition) < 0) { %>
			<% field = I18n.t(data[i].name.toLowerCase(),{scope: 'helpdesk_reports', defaultValue: data[i].name } ) %>
    	<li data-position='<%= data[i].position %>' data-action='pop-field' data-menu='<%= data[i].condition %>'> <%= unescape(field) %> </li>
    <% } %>
<% } %>