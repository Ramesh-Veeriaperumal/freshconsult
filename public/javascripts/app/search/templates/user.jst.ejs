<li id="<%= id %>-user" rel="searched-users" class="customer-results">
	<div class="search-list-wrap">
		<div class="avatar avatar-small pull-left">
			<img alt="" onerror="imgerror(this)" size_type="thumb" src="<%= avatar_url %>">
		</div>
		<div class="search-result-text row-fluid">
			<div class="ticket_subject search-result-content">
				<h5 class="ticket-description-tip">
					<a href="<%= path %>" class="search-title" data-pjax="#body-container"><%= name %></a>
				</h5>
				<% if (company_name !== null) { %>
					<span class="muted"><%= company_name %></span>
				<% } %>
			</div>
			<div class="search-result-info pull-right info-data">
				<div class="search-result-info-actions pull-right">
					<a href="/helpdesk/tickets/filter/requester/<%= id %>" data-parallel-url="/helpdesk/tickets/filter_options?filter_name=requester&requester_id=<%= id %>" class="search-result-info-action" data-parallel-placeholder="#ticket-leftFilter" data-pjax="#body-container" title="<%= lang.viewAllTickets %>">
						<span>
							<i class="helpdesk_ticket_icon ficon-16"></i>
						</span>
					</a>
				</div>
				<div class="search-result-info-details">
					<% if (email !== null) { %>
						<div>
							<span>
								<i class="muted ficon-mail fsize-18" />
							</span>
							<span class="emphasize ellipsis search-result-info-attr"><%= email %></span>
						</div>
					<% } %>
					<% if (phone !== null) { %>
						<div>
							<span>
								<i class="muted ficon-phone fsize-16" />
							</span>
							<span class="emphasize ellipsis search-result-info-attr"><%= phone %></span>
						</div>
					<% } %>
				</div>
				

			</div>
			
		</div>
	</div>
</li>