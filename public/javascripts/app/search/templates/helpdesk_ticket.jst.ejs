<li id="<%= id %>-tkt" rel="searched-tickets" data-id="<%= id %>">
	<div class="search-list-wrap">
		<% if (App.AdvancedSearch.has_bulk_action_privilege && App.AdvancedSearch.is_advanced_search_bulk_actions_feature_enabled && SEARCH_RESULTS_PAGE.is_tickets_page) { %>
			<div class="selection pull-left tooltip" data-placement="right"  twipsy-content-set="true"
				<% if(archive) {%>
					data-original-title="Archived ticket cannot be modified"
				<% } else { %>
					data-original-title=""
				<% } %>
			>
				<input type="checkbox" name="ids[]" value="<%= ticket_display_id %>" data-ticket-status="<%= ticket_status_id %>"
				<% if(archive) {%>
						disabled = "disabled" selectable = "false"
				<% } else { %>
						selectable = "true"
				<% } %> >
			</div>
		<% } %>
		<div class="avatar avatar-small pull-left">
			<img alt="" onerror="imgerror(this)" size_type="thumb" src="<%= avatar_url %>">
		</div> 
		<div class="search-result-text row-fluid">
		    <div class="ticket_subject search-result-content">
				<h5>
					<a href="<%= ticket_path %>" class="search-title" data-pjax="#body-container" >
						<%= subject %>  <span class="muted">#<%= ticket_display_id %></span>
					</a>
				</h5>

				<div class="info-data">
					<%= ticket_info %>
				</div>
				<p class="muted"><%= description %></p>
			</div>
			<div class="search-result-info pull-right info-data">
				<div>
					<span class="muted search-label"><%= SEARCH_RESULT_LANG["agent"] %></span>
					<span class="emphasize ellipsis result-info">
						<%= responder_name %>
					</span>
				</div>
				<div>
					<span class="muted search-label"><%= SEARCH_RESULT_LANG["status"] %></span>
					<span class="emphasize ellipsis result-info"><%= ticket_status %></span>
				</div>
				<div>
					<span class="muted search-label"><%= SEARCH_RESULT_LANG["group"] %></span>
					<span class="emphasize ellipsis result-info"><%= ticket_group %></span>
				</div>
			</div>
		</div>
	</div>
</li>