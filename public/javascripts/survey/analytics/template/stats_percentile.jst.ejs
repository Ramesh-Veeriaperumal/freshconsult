
<% if(dotClasses.length>0){ %>
	<div class="pull-left">
		<i class="<%=className%> survey-icon"></i>
		<div class="survey_dots text-center">
			<% for(var i=0; i<dotClasses.length; i++){ %>
				<i class="survey-indicator-new tooltip <%= dotClasses[i] %>" data-placement="below" title = "<%= dotValues[i] %>"></i>
			<% } %>
		</div>
	</div>
<% } else{ %>
	<i class="<%=className%> survey-icon"></i>
<% } %>
<%if(status){%>
	<div class="percentage-status-pod">
	<div class="overview-percentage"><%=percentage%>%</div>
	<div class="muted rating-status"><%=status%></div>
	</div>
<%}else{%>
	<span class="rating-percentage-left"><%=percentage%>%</span>
<%}%>
