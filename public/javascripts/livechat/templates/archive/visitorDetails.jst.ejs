<div class="editing">
	<div class="title">
		<span class="name">Visitor Detail</span> 
		<a  id = "cancel-visitor-detail" href="#" class="btn btn-primary hide">Close</a>
		<a  id = "save-visitor-detail" href="#" class="btn btn-primary hide">Save</a>
	</div>
	<form action="#">
		<input id ="visitorname" type="text" placeholder="Name" value = "<%=visitorData.name%>">
		<input id ="visitoremail" type="text" placeholder="Email" value = "<%=(visitorData.email == null || visitorData.email === "null"  )? "" : visitorData.email %>">
		<input id ="visitorphone" type="text" placeholder="Phone" value = "<%=(visitorData.phone == null || visitorData.phone === "null" )? "" : visitorData.phone %>">
	</form>
</div>
<ul class="info">
	<% if(visitorData.custom_field1 && visitorData.custom_field1 != "null"){ %> <li> Textfield : <%=visitorData.custom_field1 %></li> <% } %>
	<% if(visitorData.custom_field2 && visitorData.custom_field2 != -1 && visitorData.custom_field2 != "null") { %> <li>Dropdown : <%=visitorData.custom_field2 %></li> <% } %>
	<li><i class="ficon-map-marker"></i><%=(visitorData.location == null )? "Unknown" : visitorData.location %></li>
	<% if(visitorData){ %>
		<% if(visitorData.ip_address){ %>
			<li class="lc-info-ip"><%= visitorData.ip_address %></li>
		<% } %>
		<li><i class="ficon-web-page"></i><a href="<%=visitorData.visited_page%>" alt="<%=visitorData.visited_page%>" title="<%=visitorData.visited_page%>" target="_blank"><% if(visitorData.title){%><%=visitorData.title%><%}else{%><%=visitorData.visited_page%><%}%></a></li>
	<%}%>
	<% if(visitorData.browser_family){
	    print('<li><i class="ficon-'+visitorData.browser_family.toLowerCase()+'"></i>');
	    if(_.isEmpty(visitorData.browser_version)){
	      print(visitorData.browser_family);
	    }else{
				print(visitorData.browser_family+" " +visitorData.browser_version);
	    }
	    print("</li>");
	  } else {
	    print("<li><i class='ficon' ></i>Browser Unknown</li>");
	  }

	 if(visitorData.host_os_family){
	    print('<li><i class="ficon-'+visitorData.host_os_family.toLowerCase()+'"></i>');
	    if(_.isEmpty(visitorData.host_os_version)){
	      print(visitorData.host_os_family);
	    }else{
				print(visitorData.host_os_family+" " +visitorData.host_os_version);
	    }
	    print("</li>")
	  } else {
	    print("<li><i class='ficon' ></i>OS Unknown</li>");
	  } %>

</ul>
<hr>
<div class="recent-chats" id="recent-chats">
	<% if(recentChats && recentChats.length > 0){%>
	<h5>RECENT CHATS</h5>
	<ul>
		<% for(var i = 0; i < recentChats.length ;i++){ %>
		<li>
			<div class='title'>
				<a id="navigateMessage" href="#" data-id="<%=recentChats[i].chat_id%>">
					Chat With <%=window.userCollection.getTitle(parseInt(recentChats[i].agent_id))%></a>
			</div>
			<span><%=window.moment(recentChats[i].created_at).fromNow()%></span>
		</li>	
		<% } %>
	</ul>
	<%}else{ %>
		<div class ="no-data"> No Recent Chats </div>
	<% } %>
</div>
