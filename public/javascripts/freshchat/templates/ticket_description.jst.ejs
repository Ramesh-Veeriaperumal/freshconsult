<tr style="vertical-align:top; border-top: 1px solid #eee; <%= obj.cls ? obj.cls : '' %>">
	<td style="padding:10px; width:50px; border:0">
    <img src="<%=obj.photo%>" style="border-radius: 4px; width: 30px; float: left; border: 1px solid #eaeaea; max-width:inherit" alt="" />
	</td>
  <td style="padding:10px 0; width: 80%; border:0">
			<b style="color:#666;"><%=obj.name%></b>
			<p style="margin:2px 0 0 0; line-height:18px; color:#777;"><%=obj.msg%></p>
	</td>
	<td style="padding:10px; font-size:10px; color:#aaa; text-align:right; min-width:50px; border:0"><%= obj.date %></td>
</tr>
