  signing_key: <%= config[:signing_key] %>
  jwt_default_expiry: <%= config[:jwt_default_expiry] %>
  email_cipher_key: <%= config[:email_cipher_key] %>
  service_url: <%= config[:service_url] %>
<% end %>
