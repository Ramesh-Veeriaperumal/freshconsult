    sqs_queues['search_etl_queue'] = node[:ymls][:archive_sqs_queues][:search_etl_queue]
    sqs_queues['analytics_etl_queue'] = node[:ymls][:archive_sqs_queues][:analytics_etl_queue]
    sqs_queues['search_etlqueue_maintenance'] = node[:ymls][:archive_sqs_queues][:search_etl_queue]
    sqs_queues['analytics_etl_queue_maintenance'] = node[:ymls][:archive_sqs_queues][:analytics_etl_queue]
  end
%>

<%= { node[:opsworks][:environment] => sqs_queues }.to_yaml %>
