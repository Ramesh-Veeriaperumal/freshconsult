  service_name: <%= node[:ymls][:iris][:service_name] %>
  socketioUrl: <%= node[:ymls][:iris][:socketiourl] %>
  rtsUrl: <%= node[:ymls][:iris][:rts_url] %>
  irisUrl: <%= node[:ymls][:iris][:iris_url] %>
