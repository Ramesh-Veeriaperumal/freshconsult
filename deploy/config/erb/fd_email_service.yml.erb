<% config = node[:ymls][:fd_email_service] %>
<%= node[:opsworks][:environment] %>:
  key: <%= config[:key] %>
  lang_key: <%= config[:lang_key] %>
  timeout: <%= config[:timeout] %>
  email_send_urlpath: <%= config[:url] %>
  email_campaign_urlpath: <%= config[:campaign_url] %>
  account_validate_urlpath: <%= config[:account_validate_url] %>
  sender_config_urlpath: <%= config[:sender_config_url] %>
  get_domains: <%= config[:get_domains_url] %>
  configure_domain: <%= config[:configure_domain_url] %>
  verify_domain: <%= config[:verify_domain_url] %>
  remove_domain: <%= config[:remove_domain_url] %>
  antivirus_urlpath: <%= config[:antivirus_url] %>
  account_destroy_urlpath: <%= config[:account_destroy_url] %>
  lang_detect_path: <%= config [:lang_detect_path] %>
  ip_pool_category: <%= config[:ip_pool_category] %>
  incoming_email_api_key1: <%= config[:incoming_email_api_key1] %>
  incoming_email_api_key2: <%= config[:incoming_email_api_key2] %>
  imap_application_id: <%= config[:imap_application_id] %>
  host: <%= config[:host] %>
  lang_host: <%= config[:lang_host] %>
  ip_pool_names:
    - freshworks_email_high
    - freshworks_email_low
