    auth_type: <%= node[:ymls][:channel_api_keys][:proactive][:auth_type]%>
    jwt_secret: <%= node[:ymls][:channel_api_keys][:proactive][:jwt_secret]%>
  ocr_channel:
    auth_type: <%= node[:ymls][:channel_api_keys][:ocr_channel][:auth_type]%>
    jwt_secret: <%= node[:ymls][:channel_api_keys][:ocr_channel][:jwt_secret]%>
    jwt_default_expiry: <%= node[:ymls][:channel_api_keys][:ocr_channel][:jwt_default_expiry]%>
  freshmover:
    auth_type: <%= node[:ymls][:channel_api_keys][:freshmover][:auth_type]%>
    jwt_secret: <%= node[:ymls][:channel_api_keys][:freshmover][:jwt_secret]%>
