<%= render :partial => "freshfone_admin_calls" %>
<%= javascript_tag do %>
	if (typeof freshfone === "undefined") { freshfone = {}; }
	freshfone.CALL_HISTORY_CUSTOM_SEARCH_PATH = '<%= custom_search_freshfone_call_history_path %>';
	freshfone.CALL_HISTORY_CHILDREN_PATH = '<%= children_freshfone_call_history_path %>';
	freshfone.blockNumberText = '<%= t('freshfone.call_history.block_number') %>';
	freshfone.unblockNumberText = '<%= t('freshfone.call_history.unblock_number') %>';
	scriptLoader.load('360player');
<% end %>

<%= javascript_include_tag :freshfone_call_history %>

<%= stylesheet_link_tag :freshfone_call_history %>