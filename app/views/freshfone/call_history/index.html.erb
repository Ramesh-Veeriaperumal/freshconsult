<%= render :partial => "freshfone_admin_calls" %>
<%= javascript_tag do %>
	if (typeof freshfone === "undefined") { freshfone = {}; }
	freshfone.CALL_HISTORY_CUSTOM_SEARCH_PATH = '<%= custom_search_freshfone_call_history_index_path %>';
	freshfone.CALL_HISTORY_CHILDREN_PATH = '<%= children_freshfone_call_history_index_path %>';
	freshfone.blockNumberText = '<%= t('freshfone.call_history.block_number') %>';
	freshfone.unblockNumberText = '<%= t('freshfone.call_history.unblock_number') %>';
	Fjax.Assets.plugin('360player');

	freshfone.requester_autocomplete_path = '<%= requester_search_freshfone_autocomplete_index_path %>';
	freshfone.freshfone_number_list = <%= freshfone_numbers_options.html_safe %>;
	freshfone.call_status_list = <%= filtered_by_call_status_options.html_safe%>;
	freshfone.date_picker_labels = <%= date_picker_labels.html_safe %>;
	freshfone.group_list = <%= groups_options.html_safe %>;
	freshfone.customer_autocomplete_path = '<%= customer_phone_number_freshfone_autocomplete_index_path %>';
<% end %>

<%= stylesheet_link_tag_with_rtl :'cdn/ui_daterangepicker', :media => "screen" %> 
<%= javascript_include_tag :'cdn/report_date' %>
