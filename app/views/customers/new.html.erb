<h3 class="title"><%=t('company.new')%></h3>
<% form_for(@customer) do |f| %>
	<%= render :partial => "form", :locals => { :f => f } %>
	<div class="button-container">
	   <%= f.submit t('save'), {:class => "uiButton"} %> 
	   <%= link_to t('cancel'), customers_path(), :class => "uiButton grey" %>
	</div>
<% end %>