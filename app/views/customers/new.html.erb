<h3 class="title">New Company</h3>
<% form_for(@customer) do |f| %>
	<%= render :partial => "form", :locals => { :f => f } %>
	<div class="button-container">
	   <%= f.submit 'Save', {:class => "uiButton"} %> 
	   <%= link_to 'Cancel', customers_path(), :class => "uiButton grey" %>
	</div>
<% end %>