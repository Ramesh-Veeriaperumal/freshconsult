<section class="content min-height-on-desktop rounded-6 ticket-page">
  <div class="ticket-toolbar pull-right">
    <a href="{{ portal.ticket_export_url }}" data-target="#export-modal" data-submit-label="Export to CSV"
      rel="freshdialog" title="Export ticket data to CSV">
      Export tickets
    </a>
  </div>
  <div id="ticket-filter-container">
    {% snippet filters %}
  </div>
  
  {% snippet ticket_list %}
</section>