show_ajax_flash(page)
@updated_folders.each do |folder|
	page.replace "item_"+folder.id.to_s, render(:partial => "/solution/categories/folder", :locals => {:folder => folder}).html_safe
end
page << 'jQuery(".item_ids_checkbox:checked").prop("checked", false);'
page << 'jQuery(".comm-item").removeClass("active");'
page << "App.Solutions.Folder.toggleActionsClass(true)"
page << "App.Solutions.Folder.hideSelectAll()"