show_ajax_flash page if flash[:notice].present?
if @domain_category.present?
  page.replace "dkim-list-#{@domain_category.id}", :partial => "dkim_domains", :locals => { :domain_category => @domain_category }
  page << "jQuery('#dkim-list-"+@domain_category.id.to_s+"').addClass('active')"
  page << "jQuery('#dkim-list-"+@domain_category.id.to_s+"').find('.verification-status').addClass('hide')"
end