

<% form_for :dump, :url=>{:controller=>"admin/zip_readers", :action=>"extract_zip"}, :html => { :multipart => true } do |f| -%>
 <table">
   <tr>
   	 <td>
      <label for="text">
        zendesk URL :
      </label>
     </td>
     <td >
       <%= f.text_field :url, :class => "text" %>
     </td>   	
   </tr>
   <tr>
   	 <td>
      <label for="text">
        zendesk user name :
      </label>
     </td>
     <td >
       <%= f.text_field :user_name, :class => "text" %>
     </td>   	
   </tr>
   <tr>
   	<td>
      <label for="text">
        zendesk password :
      </label>
     </td>
     <td >
       <%= f.password_field :user_pwd, :class => "password" %>
     </td>   
   </tr>
   <tr>
     <td>
      <label for="dump_file">
        Select zendesk backup zip File :
      </label>
     </td>
     <td >
       <%= f.file_field :file -%>
     </td>
   </tr>
   <tr>
     <td colspan='2'>
       <%= submit_tag 'Submit' -%>
     </td>
   </tr>
 </table>
<% end -%>