<h1>Editing portal_template_page</h1>

<% form_for(@portal_template_page) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :portal_template_id %><br />
    <%= f.text_field :portal_template_id %>
  </p>
  <p>
    <%= f.label :account_id %><br />
    <%= f.text_field :account_id %>
  </p>
  <p>
    <%= f.label :page_type %><br />
    <%= f.text_field :page_type %>
  </p>
  <p>
    <%= f.label :page_content %><br />
    <%= f.text_area :page_content %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @portal_template_page %> |
<%= link_to 'Back', portal_template_pages_path %>