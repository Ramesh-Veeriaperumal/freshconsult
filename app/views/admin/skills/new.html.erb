<% content_for :helpcard do %>
	<%= render :partial => "helpnote_without_tour" %>
<% end %>

<h3 class="lead"><%=t('.new')%></h3>
<%= form_for @skill, :url => admin_skills_path, :html => { :method => :post , :id => "NewSkill" } do |f| %>
	<%= render :partial => "/admin/skills/rule_form", :locals => { :f => f } %>
<% end %>
<%= render :partial => '/admin/shared/popup' %>
<%= javascript_tag do %>
  (function($) {

  	$(window).on('load', function(){
      window.App = window.App || {};
      App.Admin = App.Admin || {};
      App.Admin.Skills = App.Admin.Skills || {};
      App.Admin.Skills.New.init();
    });

  })(jQuery);

<% end %>
