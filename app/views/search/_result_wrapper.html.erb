<% if current_account.features?(:advanced_search)  %>
<ul class="unstyled" id="search-page-results">
</ul>
<% else %>
<ul class="unstyled menuselector" id="search-page-results">
</ul>
<% end %>
<% if !current_filter.eql?('tickets') %>
	<%= load_more({
	  :totalPages   => @total_pages,
	  :url          => %{/search/#{current_filter}},
	  :params       => { :term => h(@search_key) },
	  :dataType     => 'json',
	  :complete 	=> 'search_page.loadMoreRender(data)',
	  :container => '#search-page-results',
	  :currentPage => @current_page,
	  :loadMoreHtml => %{<div class="loadmore-wrap"><a id="load-more" data-loading-text="<span class='sloading loading-small'></span>">
	  	#{t('livechat.chat_loadmore')}</a></div>} }) %>
<% else %>
	<div class="loadmore-wrap hide">
		<a id="load-more-with-filters" data-loading-text="<span class='sloading loading-small'></span>">
		  	<%= t('livechat.chat_loadmore') %></a>
	</div> 
<% end %>
