<a data-agent-id="-" data-ticket-id="<%= ticket_id %>" class="action_assign_agent" data-value="-" href=""><%= t("ticket.unassigned") %></a>
<a data-agent-id="me" data-ticket-id="<%= ticket_id %>" class="action_assign_agent" data-value="<%= current_user.name %>" href=""><%= t("ticket.me") %></a>
<span class="seperator"></span>
<% if @agents.blank? %>
  <a class="disabled"><%= t("no_agents_in_selected_group") %></a>
<% else %>
  <% @agents.each do |agent| %>
	  <a data-agent-id="<%=agent.user.id%>" data-ticket-id="<%= ticket_id %>" class="action_assign_agent" href=""><%=agent.user.name%></a>
  <% end %>
<% end %>
