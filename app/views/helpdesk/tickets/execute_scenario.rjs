f = ActionView::Helpers::FormBuilder.new(:helpdesk_ticket, @ticket, self, {}, proc {})
page.replace_html 'TicketPropertiesFields',
	render(:partial => "helpdesk/tickets/show/ticket_field_elements.html.erb",
		:locals => { 
			:form_builder => ActionView::Helpers::FormBuilder.new(:helpdesk_ticket, @ticket, self, {}, proc {}) 
		})
