<div data-gs-x="<%= x %>" data-gs-y="<%= y %>" data-gs-width="<%= width %>" data-gs-height="<%= height %>" class="grid-stack-item widget">
	<div class="grid-stack-item-content widget-inner content">
			<div class="metric-head">
					<div class="widget-metric-title pull-left"><%= t("helpdesk.realtime_dashboard.unresolved") %></div>
					<div class="controls pull-right">
						<i class="ficon-pencil icons tooltip fsize-16" data-placement="above" data-original-title="Edit" twipsy-content-set="true"
						href="#widget-unresolved-edit" 
						rel="freshdialog" 
						data-backdrop="static"
						data-show-close="false"
						data-submit-label="Update" 
						data-width="400"
						></i>
						<i class="ficon-refresh icons tooltip fsize-16" data-placement="above" data-original-title="Refresh" twipsy-content-set="true"></i>
					</div>
					<div class="clear"></div>
			</div>
			<div class="metric-content">
				<div class="indicator">
					<i class="ficon-infinity-5"></i>
				</div>
				<div class="value">142</div>
			</div>		
	</div>
</div>
<div id="widget-unresolved-edit" class="hide row-fluid">
	<div class="dialog_container">
			<div class="widget-edit">
			      <div class="form">
			      	 <div class="form-row">
			            <label><%= t("helpdesk.realtime_dashboard.title") %></label>
			            <div class="field">
			               <input class="title" type="text" />
			            </div>
			         </div>
			         <div class="form-row">
				         <div class="message"><%= t("helpdesk.realtime_dashboard.critical_limits") %></div>
				     </div>
			         <div class="form-row">
			         		<label class="text-right"><%= t("agents") %></label>
            				<div class="field">
            					<input type="text">
            				</div>
            				<label class="text-right"><%= t("helpdesk.realtime_dashboard.supervisors") %></label>
            				<div class="field">
            					<input type="text">
            				</div>
			         </div>
			      </div>
			</div>
	</div>
</div>
