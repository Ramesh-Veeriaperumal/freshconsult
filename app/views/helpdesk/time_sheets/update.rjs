unless @new_show_page
	page << "timesheet.editCompelete('add', true)"
	page.replace "timeentry_#{@time_entry.id}", :partial => "/helpdesk/time_sheets/time_entry", :object => @time_entry

	pushToTimesheetIntegratedApps page, @time_entry

else
	page << render(:partial => "helpdesk/time_sheets/v2/update")
end