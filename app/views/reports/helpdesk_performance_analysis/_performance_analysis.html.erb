<div class="row-fluid">
  <div class="span12">
    <h3 class="header-lead">
      <span><%=t('adv_reports.load.resp_accuracy')%></span>
      <a class="report-help-icon tooltip" title='<%=t("adv_reports.helptext.glance.how_to_read")%>' href="<%=@solution_artical_link%>" target="_blank">?</a>
    </h3>
    <div id="response_accuracy_line_chart" class="single-stacked-bar" style="height:250px;"><div class="noinfo"><%=t('adv_reports.no_data_to_display_msg')%></div></div>
  </div>
</div>
<div class="row-fluid">
  <div class="span12">
    <h3 class="header-lead">
      <span><%=t('adv_reports.load.resp_time')%></span>
      <a class="report-help-icon tooltip" title='<%=t("adv_reports.helptext.glance.how_to_read")%>' href="<%=@solution_artical_link%>" target="_blank">?</a>
    </h3>
    <div id="response_time_line_chart" class="single-stacked-bar" style="height:250px;"><div class="noinfo"><%=t('adv_reports.no_data_to_display_msg')%></div></div>
  </div>
</div>
<div class="row-fluid">
  <div class="span12">
    <h3 class="header-lead">
      <span><%=t('adv_reports.glance.avg_cust_intr')%> vs <%=t('adv_reports.glance.avg_agent_intr')%></span>
      <a class="report-help-icon tooltip" title='<%=t("adv_reports.helptext.glance.how_to_read")%>' href="<%=@solution_artical_link%>" target="_blank">?</a>
    </h3>
    <div id="interactions_line_chart" class="single-stacked-bar" style="height:250px;"><div class="noinfo"><%=t('adv_reports.no_data_to_display_msg')%></div></div>
  </div>
</div>
<div class="row-fluid">
  <div class="span12">
    <h3 class="header-lead">
      <span><%=t('adv_reports.load.tkt_assignment')%></span>
      <a class="report-help-icon tooltip" title='<%=t("adv_reports.helptext.glance.how_to_read")%>' href="<%=@solution_artical_link%>" target="_blank">?</a>
    </h3>
    <div id="assignments_line_chart" class="single-stacked-bar" style="height:250px;"><div class="noinfo"><%=t('adv_reports.no_data_to_display_msg')%></div></div>
  </div>
</div>
  
<script type="text/javascript">
var data_obj = <%=@data_obj.to_json.html_safe%>
//need to check for better approach
if(_slide_init == false){
  jQuery('#sliding').slide();
  _slide_init = true;
}
jQuery(document).ready(function() {
  var analysis_1_lineChart = new lineChart(
      {renderTo: 'response_accuracy_line_chart',
       chartData:[data_obj['resolved_tickets'],data_obj['fcr_tickets'],data_obj['sla_tickets'],data_obj['num_of_reopens']],
       yAxisTitle:"<%=t('adv_reports.load.num_of_tickets')%>"}
    );
  analysis_1_lineChart.lineChartGraph();
  
  var analysis_2_lineChart = new lineChart(
      {renderTo: 'response_time_line_chart',
      chartData:[data_obj['first_response_time'],data_obj['avg_response_time']],
      yAxisTitle:"<%=t('adv_reports.load.time_in_hrs')%>"}
    );
  analysis_2_lineChart.lineChartGraph();
  var analysis_3_lineChart = new lineChart(
      {renderTo: 'interactions_line_chart',
      chartData:[data_obj['customer_interactions'],data_obj['agent_interactions']],
      yAxisTitle:"<%=t('adv_reports.load.avg_num_of_interactions')%>"}
    );
  analysis_3_lineChart.lineChartGraph();
  var analysis_4_lineChart = new lineChart(
      {renderTo: 'assignments_line_chart',
      chartData:[data_obj['tickets_assigned'],data_obj['num_of_reassigns']],
      yAxisTitle:"<%=t('adv_reports.load.num_of_operations')%>"}
    );
  analysis_4_lineChart.lineChartGraph();

});
</script>