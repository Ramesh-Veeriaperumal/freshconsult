<div class="ticket-list-wrapper hide" id="ticket_list_wrapper">
	<div class="list-title">
		<div class="title_wrapper">	
			<div class="metric_title"></div>
			<div class="metric_value"></div>
		</div>
		<div class="cross" data-action="hide-ticket-list"><%= font_icon "cross", :size => 20 %></div>
		<% if current_user.privilege?(:export_reports) %>
			<div class="export_title pull-right hide">
				<a class="link" href="javascript:void(0);" ><%= t('export_data.view_all_tickets') %></a>
				<span class="title hide"><%= t('export_data.view_all_tickets') %></span> 
			</div>
		<% end %>
		<div class="clear"></div>
	</div>
	<%= render :partial => "reports/v2/tickets/reports/shared/export" %> 
	<div class="sla-toggle-tab hide">
		<div class="link1">
			<span class="toggle active" data-action="toggle-ticketlist" ></span>
		</div>
		<div class="link2">
			<span class="toggle" data-action="toggle-ticketlist" ></span>
		</div>
	</div>
	<div id="ticket_list"></div>
</div>	