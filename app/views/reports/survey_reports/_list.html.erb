	  <ul class="table-list">
		  <%@reports_list.each do |key,value|%>
		   <li>
			<%= link_to_function ((render :partial => "/reports/survey_reports/list_content", :locals => {:content => value,:is_list => true})  ,"fetch_details('"+value[:entity_id].to_s+"','"+value[:category]+"')", {:class => "agent_report_list"})  %>
			</li>	
		  <%end%>
	  </ul>
  
<%if @reports_list.blank?%>
	 <h1 style="font-size:18px;text-align:center;margin-top:50px;"> <%=t('.no_feedback')%> </h1>
<%end%>

<% javascript_tag do %>
	jQuery("#date_range_title").html("<%=params['date_range']%>")

	<%if @reports_list.blank?%>
			jQuery("#back_to_con").hide();
	<%end%>	

	function fetch_details(entity_id,category){		
		var details_form = [ '<form method="POST" action="<%=survey_report_details_url%>">' ];
    			 	details_form.push('<input type="hidden" name="category" value="'+category+'"/>');
    			 	details_form.push('<input type="hidden" name="entity_id" value="'+entity_id+'"/>');
    			 	details_form.push('</form>');

    			 jQuery(details_form.join('')).appendTo('body')[0].submit();
	}
<%end%>