<%= javascript_include_tag  'highcharts.js' %>
<%= javascript_include_tag  '/javascripts/report/date' %>
<%= javascript_include_tag  '/javascripts/report/daterangepicker.jQuery.js' %>
<%= include_cloudfront_css :reports, :media => "screen, print, projection"  %>
<%= include_cloudfront_css :report_print, :media => "print, projection" %>
<%= stylesheet_link_tag 'ui.daterangepicker' %>
<% content_for :sidebar do %>
	<%= render :partial => "/reports/helpdesk_reports/sidebar" %>
<%end%>	
<div class="report-container" id="report-container"></div>            
                                           
<% content_for :onload_script do %>     
  jQuery("#Sidebar form").submit();
  jQuery('#date_range').bind('keypress keyup keydown', function(ev) {
  	ev.preventDefault();
  	return false;
  });
<% end %>
