<% content_for :sidebar do %>
  <%= render :partial => "helpnote_without_tour" %>
<% end %>
<h3 class="title"><%=t('agent.new')%></h3>
<% form_for @agent, :html => { :multipart => true } do |f| %>
	<%= f.error_messages %>	
 	<%= render :partial => "agent_form", :locals => { :type => "new" } %>
	<div class="button-container">
   		<%= f.submit t('save'), { :class => "uiButton" } %>
		<%= link_to t('cancel'), {:action => 'index'}, :class => "uiButton" %>
	</div>  
<%end%>