<h3 class="title">Edit Agent</h3>
<% form_for @agent, :html => { :multipart => true } do |f| %>
	<%= f.error_messages %>
	<%= render :partial => "agent_form", :locals => { :type => "edit" } %>
	<div class="button-container">
   		<%= f.submit "Update", { :class => "uiButton" } %>
		<%= link_to "Cancel", {:action => 'index'}, :class => "uiButton" %>
	</div>	  
<%end%>

 