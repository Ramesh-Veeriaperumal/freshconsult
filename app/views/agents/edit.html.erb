<div class="pageContent">
	<h3 class="content_title">Edit Agent</h3>
	<span class="seperator"></span>
	<% form_for @agent, :html => { :multipart => true } do |f| %>
		<%= f.error_messages %>
		<%= render :partial => "agent_form", :locals => { :type => "edit" } %>
		<div class="button-container">
    		<%= f.submit "Update", { :class => "fdbutton" } %>
			<%= link_to "Cancel", {:action => 'index'}, :class => "fdbutton grey" %>
  		</div>	  
	<%end%>
</div>
 