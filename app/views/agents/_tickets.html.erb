<ul class="history-list">
	<% tickets.each do |ticket| %>
	   <li>
	   	<%= link_to("##{ticket.display_id} " + (ticket.subject), "/a/tickets/#{ticket.display_id}", :class => "title", target: "_top") %>
		<div class="info_data">
			<%= formated_date(ticket.created_at) %>
			| <%=t('ticket.status')%> <%= ticket.status_name %>
		</div>
	   </li>
	<% end %>
</ul>