<ul class="history-list">
	<% tickets.each do |ticket| %>
	   <li>
	   	<%= link_to("##{ticket.display_id} " + (ticket.subject), ticket, :class => "title", :'data-pjax' => '#body-container') %>
		<div class="info_data">
			<%= formated_date(ticket.created_at) %>
			| <%=t('ticket.status')%> <%= ticket.status_name %>
		</div>
	   </li>
	<% end %>
</ul>