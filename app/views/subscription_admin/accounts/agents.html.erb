<h1><%= @page_title = 'Agents' %></h1>
<table id="accounts">
  <tr>
    <th>Name</th>
    <th>Domain</th>
    <th>Created</th>
	<th>Email</th>
	<th>Agent Count</th>
  </tr>
  <%unless @accounts.blank?%>
  <% @accounts.each do |account| %>
    <tr>
      <td><%= link_to(h(account.name), [:admin, account]) %></td>
      <td><%= account.full_domain %></td>
      <td><%= account.created_at.to_s(:short_day) %></td>
	  <td><%= account.account_admin.email %></td>
	  <td><%= account.agents.count %></td>
    </tr>
  <% end %>
  <%end%>
</table>

<%= will_paginate(@accounts) %>