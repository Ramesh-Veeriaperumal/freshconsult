<head>
	<script type="text/javascript">
    var t_pagestart=new Date().getTime();
  </script>
  <meta charset="utf-8">
	<!-- Always force latest IE rendering engine (even in intranet) & Chrome Frame 
	     Remove this if you use the .htaccess -->
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">  
  <title><%= page_title %></title>
	<meta name="description" content="<%= page_description %>">
	<meta name="author" content="">                            
	<meta name="keywords" content="<%= page_keywords %>">
		
	<!--  Mobile viewport optimized: j.mp/bplateviewport -->
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	
	<link rel="shortcut icon" href="<%=current_portal.fav_icon.nil? ? '/images/favicon.ico' : current_portal.fav_icon.content.url%>" />
	<%= include_stylesheets :common  %>
	<%= include_stylesheets :app  %>
	<%= include_stylesheets :notifications %>
	<%= render :partial => "shared/theme.css"  %>	
	<%= include_javascripts :defaults  %>
	<%= include_javascripts :frameworks %>
	<%= include_javascripts :workspace %>
	<%= include_javascripts :custom_widget %>
	
	<%= yield :head %>
	<style>
		#benchmarkresult{
			width: 25%;
			display: inline-block;
			position: fixed;
			z-index: 999;
			background: #DFDFDF;
			bottom: 10px;
			right: 10px;
			border: 1px solid gainsboro;
			padding: 10px;
			box-shadow: 2px 2px #AAA;
			color: #333;
			border-radius: 5px;
		}
	</style>
</head>
<div id="benchmarkresult">
</div>