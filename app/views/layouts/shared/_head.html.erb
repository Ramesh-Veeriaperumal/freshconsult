<head>
	<% if @debuging%>
		<script type="text/javascript">
	    	var t_pagestart=new Date().getTime();
	  	</script>
  	<%end%>
  	<meta charset="utf-8">
	<!-- Always force latest IE rendering engine (even in intranet) & Chrome Frame
	     Remove this if you use the .htaccess -->
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  	<title><%= h(page_title) %></title>
	<meta name="description" content="<%= page_description %>">
	<meta name="author" content="">
	<meta name="keywords" content="<%= page_keywords %>">
	<!-- Adding meta tag for CSRF token -->
	<%= csrf_meta_tag %>
	<!-- End meta tag for CSRF token -->
	<!--  Mobile viewport optimized: j.mp/bplateviewport -->
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<% unless page_canonical.blank? %>
		<link rel="canonical" href="<%= page_canonical %>" />
	<% end %>
	<link rel="canonical shortcut icon" href="<%= fav_icon_url %>" />
	<%= include_cloudfront_rtl_css :common %>
	<%= include_cloudfront_rtl_css :pattern, :media => "screen, print, projection"  %>
	<%= include_cloudfront_rtl_css :plugins, :sprites, :app, :non_pattern, :tour_my_app %>
	<%= include_cloudfront_rtl_css :ie_hacks %>
	<%= include_cloudfront_rtl_css :tkt_details, :media => "screen, print, projection"  %>
	<%= include_cloudfront_rtl_css :'player360' %>
	<%= include_cloudfront_rtl_css :community %>

	<%= include_cloudfront_rtl_css :'freshfone_recording_playback' %>
	<%#= render :partial => "shared/theme.css"  %>

	<script type="text/javascript">
		PROFILE_BLANK_THUMB_PATH = '<%= image_path("fillers/profile_blank_thumb.gif") %>';
		PROFILE_BLANK_MEDIUM_PATH = '<%= image_path("fillers/profile_blank_medium.gif") %>';
		SPACER_IMAGE_PATH = '<%= image_path("spacer.gif") %>';
		FILLER_IMAGES = {
			imageLoading : '<%= image_path("fillers/image_upload_placeholder.gif") %>'
		};
		cloudfront_version = "<%= CLOUDFRONT_VERSION %>";
		cloudfront_host_url = "<%= CloudfrontAssetHost.asset_host('frameworks.js',request) %>";
	</script>
	<%= include_cloudfront_js :defaults  %>
	<%= include_cloudfront_js :frameworks %>
	<%= include_cloudfront_js :workspace %>
	<%= include_cloudfront_js :redactor %>
	<%= include_cloudfront_js :shortcut if shortcuts_enabled? %>
	<%= include_cloudfront_js :syntaxhighlighter %>


	<%= include_cloudfront_js :pattern  %>
	<%= include_cloudfront_js :custom_widget %>

	<% if current_account.features?(:freshfone) ||
	  (current_account.features?(:chat) && current_account.chat_setting && !(is_assumed_user?)) %>
		<%= include_cloudfront_js :soundmanager %>
	<% end %>
	<% if chat_activated? && !current_account.features_included?(:collision_socket) %>
		<%= include_cloudfront_js :freshchat %>
	<% end %>
	
	<% if chat_activated? %>
		<%= include_cloudfront_js :freshchat_intializer %>
	<% end %> 

	<%= include_cloudfront_js_langs %>

	<script type="text/javascript">
		Fjax.pjax_parallel_request = null;
		var js_assets = JSON.parse('<%= Jammit.configuration[:javascripts].to_json =%>'.replace(/&quot;/g,'"')),
				css_assets = JSON.parse('<%= Jammit.configuration[:stylesheets].to_json =%>'.replace(/&quot;/g,'"'));
		window.scriptLoader = new ScriptLoader(js_assets, css_assets);
		Fjax.Assets.setup(js_assets, css_assets, cloudfront_version, cloudfront_host_url);
		Fjax.Manager.start();
	</script>


	<%= yield :head %>

	<link href="<%= helpdesk_theme_url %>" media="screen" rel="stylesheet" type="text/css">
	<%= render :partial => 'layouts/shared/app_analytics' if current_user.present? and current_user.agent? and Rails.env.production? %>
</head>

<%if @debuging %>
	<style>
		#benchmarkresult{
			width: 25%;
			display: inline-block;
			position: fixed;
			z-index: 999;
			background: #DFDFDF;
			bottom: 10px;
			right: 10px;
			border: 1px solid gainsboro;
			padding: 10px;
			box-shadow: 2px 2px #AAA;
			color: #333;
			border-radius: 5px;
		}
	</style>
	<div id="benchmarkresult">
	</div>
<%end%>
