<%= javascript_include_tag :'cdn/czentrix' %>

<%= javascript_tag do %>
	if (typeof cti_user === "undefined") { cti_user = {}; }
	<% app_config = installed_app.configs[:inputs] %>
	cti_user.host_ip = '<%= app_config[:host_ip] %>';
	cti_user.hide_dont_convert = <%= app_config[:convert_to_ticket] || 0 %>;
	cti_user.email = '<%= current_user.email %>';
	cti_user.widget_name = 'czentrix_widget';
	cti_user.add_note_as_private = <%= app_config[:add_note_as_private] || 0 %>;
<% end %>