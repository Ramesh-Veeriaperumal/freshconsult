<!DOCTYPE html>
<%= render :partial => "layouts/shared/html" %>
	<head>
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

		<title><%= page_title %></title>
		<meta name="description" content="<%= page_description %>">
		<meta name="author" content="">
		<meta name="keywords" content="<%= page_keywords %>">

		<meta name="viewport" content="width=device-width">
		<%=portal_fav_ico%>
		<%#= include_stylesheets :portal, :redactor %>
		<%= include_stylesheets :portal %>
		<link href="<%=theme_url%>" media="screen" rel="stylesheet" type="text/css">
		<%= portal_fonts %>

		
		<%= include_javascripts :portal_head %>		
	</head>
	<body>
		<%= assumed_identity_message %>
		
		<%= @layout %>

		<%= include_javascripts :portal_bottom, :redactor %>
		<%= render :partial => "layouts/integrations" %>
	</body>
</html>