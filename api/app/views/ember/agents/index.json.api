if @only_omni_channel_availability
  if @proxy_to_mars
    @items.map(&:record)
  else
    @items.map(&:channel_agents_availability_data)
  end.to_json
else
  @items.map do |agent|
    @availability_details.present? ? agent.availability_hash(@availability_details[agent.record.user_id]) : (@privilege_filter ? agent.to_privilege_hash : agent.to_hash(@day_pass_used_count))
  end.to_json
end
